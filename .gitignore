# Ignore directories generated by Composer
drush/contrib
vendor
web/core
web/themes/custom/*/css
web/themes/custom/*/images/sprite.png
web/themes/custom/*/images/sprite@2x.png
web/themes/custom/subthemes/*/css
web/modules/contrib
web/libraries
web/themes/contrib
web/sites/sites.php
web/themes/custom/*/scss/base/_sprites.scss

# Ignore paths that contain user-generated content.
web/sites/*/files
web/sites/*/private

# Ignore SimpleTest multi-site environment.
webroot/sites/simpletest

# Ignore gulp things
node_modules
.sass-cache
.eyeglass_cache
*.css.map

# Other useful ignore stuff
.DS_Store
.DS_Store?
.idea
npm-debug.log

# Ignore configuration files that may contain sensitive information.
web/sites/*/settings.local.php
web/sites/*/settings.php
web/sites/settings*.php
web/sites/*/services*.yml

# Ignore any greylist item
config/test_site_overrides/*.yml
config/multi_site_overrides/*.yml
.gitattributes

# Files from the Drupal scaffold for composer.
/.editorconfig
web/.csslintrc
web/.eslintignore
web/.eslintrc.json
web/.ht.router.php
web/.htaccess
web/example.gitignore
web/index.php
web/INSTALL.txt
web/README.txt
web/robots.txt
web/update.php
web/web.config
web/sites/README.txt
web/sites/development.services.yml
web/sites/example.settings.local.php
web/sites/example.sites.php
web/sites/default/default.services.yml
web/sites/default/default.settings.php
web/modules/README.txt
web/profiles/README.txt
web/themes/README.txt
